AC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACZ","file":"/app/app/src/components/adminPage/user/UserDetail.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"mx-xl-5\">\n    <div class=\"mt-4\">\n      <a href=\"#\" @click=\"transitionList\">ユーザー一覧画面</a>\n      <span> > 詳細画面</span>\n    </div>\n    <div class=\"mt-5\">\n      <h2>詳細画面</h2>\n      <div>\n      </div>\n      <div class=\"mt-5\">\n        <div class=\"d-flex align-items-center\">\n          <div>\n            <img\n            src=\"@/image/人物のアイコン素材.jpeg\"\n            alt=\"\"\n            class=\"select-user-icon\"\n            />\n          </div>\n          <div class=\"w-100\">\n            <div>\n              <ul class=\"d-flex flex-wrap justify-content-between align-items-center\">\n                <li class=\"count\">\n                  <div>\n                    <p>投稿数</p>\n                    <span>{{ refState.user.post_count }}</span>\n                  </div>\n                </li>\n                <li class=\"count\">\n                  <div>\n                    <p>いいね数</p>\n                    <span>{{ refState.user.like_count }}</span>\n                  </div>\n                </li>\n                <li class=\"count\">\n                  <div>\n                    <p>フォロー数</p>\n                    <span>{{ refState.user.follow_count }}</span>\n                  </div>\n                </li>\n                <li class=\"count\">\n                  <div>\n                    <p>フォロワー数</p>\n                    <span>{{ refState.user.follower_count }}</span>\n                  </div>\n                </li>\n                <li class=\"count\">\n                  <div>\n                    <p>メッセージ数</p>\n                    <span>{{ refState.user.message_user_count }}</span>\n                  </div>\n                </li>\n              </ul>\n            </div>\n            <div>\n              <ul class=\"d-flex flex-wrap justify-content-between align-items-center\">\n                <li class=\"count\">\n                <div>\n                  <p>ユーザーID</p>\n                  <span>{{ refState.user.user_id }}</span>\n                </div>\n              </li>\n              <li class=\"count\">\n                <div>\n                  <p>ユーザー名</p>\n                  <span>{{ refState.user.user_name }}</span>\n                </div>\n              </li>\n              <li class=\"count\">\n                <div>\n                  <p>メールアドレス</p>\n                  <span>{{ refState.user.email }}</span>\n                </div>\n              </li>\n              <li class=\"count\">\n                <div>\n                  <p>電話番号</p>\n                  <span>{{ refState.user.phone_number }}</span>\n                </div>\n              </li>\n              <li class=\"count\">\n                <div>\n                  <p>生年月日</p>\n                  <span>{{ refState.user.birthday }}</span>\n                </div>\n              </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"mt-5 d-flex\">\n        <div class=\"w-50\">\n          <div>自己紹介</div>\n            <span>{{ refState.user.self_introduction }}</span>\n        </div>\n        <div class=\"w-50\">\n          <ul class=\"d-flex flex-wrap justify-content-between\">\n            <li class=\"notification-related\">\n              <div>いいね通知</div>\n              <span>{{ judgeFlag(refState.user.can_like_notification) }}</span>\n            </li>\n            <li class=\"notification-related\">\n              <div>コメント通知</div>\n              <span>{{ judgeFlag(refState.user.can_comment_notification) }}</span>\n            </li>\n            <li class=\"notification-related\">\n              <div>メッセージ通知</div>\n              <span>{{ judgeFlag(refState.user.can_message_notification) }}</span>\n            </li>\n            <li class=\"notification-related\">\n              <div>カレンダー通知</div>\n              <span>{{ judgeFlag(refState.user.can_calender_notification) }}</span>\n            </li>\n          </ul>\n          <ul class=\"d-flex flex-wrap justify-content-between\">\n            <li class=\"others-data\">\n              <div>削除</div>\n              <span>{{ refState.user.is_delete }}</span>\n            </li>\n            <li class=\"others-data\">\n              <div>登録日</div>\n              <span>{{ refState.user.created_at }}</span>\n            </li>\n            <li class=\"others-data\">\n              <div>更新日</div>\n              <span>{{ refState.user.updated_at }}</span>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, reactive, onMounted } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nimport { User } from \"@/types/User\";\n\ninterface UserExtend extends User {\n  post_count: number;\n  like_count: number;\n  follow_count: number;\n  follower_count: number;\n  message_user_count: number;\n}\n\ninterface State {\n  user: UserExtend;\n}\n\nexport default defineComponent({\n  props: {\n    id: {\n      type: String,\n      default: \"\",\n    },\n  },\n\n  setup(props) {\n    const router = useRouter();\n\n    const refState = reactive<State>({\n      user: {\n        post_count: 0,\n        like_count: 0,\n        follow_count: 0,\n        follower_count: 0,\n        message_user_count: 0,\n        id: 0,\n        user_name: \"\",\n        user_id: \"\",\n        self_introduction: \"\",\n        email: \"\",\n        phone_number: 0,\n        birthday: new Date(),\n        image: \"\",\n        can_like_notification: false,\n        can_comment_notification: false,\n        can_message_notification: false,\n        can_calender_notification: false,\n        is_delete: false,\n        created_at: new Date(),\n        updated_at: new Date(),\n      },\n    });\n\n    onMounted(async () => {\n      const result = await axios.get(\"http://localhost:3000/users/\" + props.id);\n      refState.user = result.data[0];\n      console.log(refState.user);\n    });\n\n    const transitionList = () => {\n      router.push({\n        name: \"UserList\",\n      });\n    };\n\n    const judgeFlag = (flag: boolean) => {\n      if (flag) {\n        return \"◯\";\n      } else {\n        return \"\";\n      }\n    };\n\n    const judgeDelete = (flag: boolean) => {\n      if (flag) {\n        return \"削除\";\n      } else {\n        return \"\";\n      }\n    };\n\n    return {\n      refState,\n      transitionList,\n      judgeFlag,\n    };\n  },\n});\n</script>\n\n<style scoped>\nul {\n  list-style: none;\n  padding-left: 0;\n}\n\n.count {\n  width: 10%;\n}\n\n.noti {\n  width: 30%;\n  margin-bottom: 10px;\n  margin-left: 3.3%;\n}\n\n.select-user-icon {\n  width: 100px;\n}\n\n.notification-related {\n  width: 25%;\n}\n</style>\n"]}]}