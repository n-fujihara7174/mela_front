AC,CAAC,EAAE;EACF,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACb;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAC7E,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AACd;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACR,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AACd;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AAChB","file":"/app/app/src/components/adminPage/user/UserList.vue","sourceRoot":"","sourcesContent":["<template>\n  <div id=\"UserList\" class=\"mx-xl-5\">\n    <div class=\"mb-5\">\n      <h2>ユーザー一覧</h2>\n    </div>\n    <div class=\"user-list-wrapper\">\n      <div class=\"serch-wrapper row\">\n        <div class=\"serch-user-name col-1\">\n          <label class=\"search-parameter-label small\">ユーザーID、メールアドレス</label>\n          <input v-model=\"refState.searchValue.userIdOrEmail\" />\n        </div>\n        <div class=\"col-xxl-2\">\n          <button @click=\"searchUsers\">検索</button>\n        </div>\n      </div>\n      <table class=\"table table-striped\">\n        <thead>\n          <tr class=\"table-header\">\n            <th class=\"id\">id</th>\n            <th class=\"user-name\">ユーザー名</th>\n            <th class=\"user-id\">ユーザーID</th>\n            <th class=\"self-introduction\">自己紹介</th>\n            <th class=\"email\">メールアドレス</th>\n            <th class=\"phone-number\">電話番号</th>\n            <th class=\"birthday\">生年月日</th>\n            <th class=\"image\">画像</th>\n            <th class=\"post-count\">投稿数</th>\n            <th class=\"like\">いいね通知</th>\n            <th class=\"comment\">コメント通知</th>\n            <th class=\"message\">メッセージ通知</th>\n            <th class=\"calender\">カレンダー通知</th>\n            <th class=\"is_delete\">削除</th>\n            <th class=\"created-at\">登録日時</th>\n            <th class=\"updated-at\">更新日時</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(user, index) in refState.users\" :key=\"index\">\n          <td>{{ user.id }}</td>\n          <td>{{ user.user_name }}</td>\n          <td>{{ user.user_id }}</td>\n          <td>{{ user.self_introduction }}</td>\n          <td>{{ user.email }}</td>\n          <td>{{ user.phone_number }}</td>\n          <td>{{ user.birthday }}</td>\n          <td>{{ user.image }}</td>\n          <th>{{ user.post_count }}</th>\n          <td>{{ judgeFlag(user.can_like_notification) }}</td>\n          <td>{{ judgeFlag(user.can_comment_notification) }}</td>\n          <td>{{ judgeFlag(user.can_message_notification) }}</td>\n          <td>{{ judgeFlag(user.can_calender_notification) }}</td>\n          <td>{{ judgeDelete(user.is_delete) }}</td>\n          <td>{{ user.created_at }}</td>\n          <td>{{ user.updated_at }}</td>\n        </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\n/* eslint-disable no-console */\nimport { defineComponent, reactive, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport {User} from \"@/types/User\"\n\n//検索\ninterface SearchValue {\n  userIdOrEmail: string;\n}\n\ninterface State {\n  searchValue: SearchValue;\n  users: Array<User>;\n}\n\nexport default defineComponent({\n  setup() {\n    const searchValueInit: SearchValue = {\n      userIdOrEmail: \"\",\n    };\n\n    const refState = reactive<State>({\n      searchValue: searchValueInit,\n      users: [],\n    });\n\n    const fetchUsers = async () => {\n      const result = await axios.get(\"http://localhost:3000/users\");\n      refState.users = { ...result.data };\n    };\n\n    onMounted(fetchUsers)\n\n    const searchUsers = async () => {\n      if (refState.searchValue.userIdOrEmail != null) {\n        const result = await axios.get(\"http://localhost:3000/users\", {\n          params: {\n            user_id_or_email: refState.searchValue.userIdOrEmail,\n          },\n        });\n        refState.users = { ...result.data };\n      }\n    };\n\n    const judgeFlag = (flag: boolean) => {\n      if (flag) {\n        return \"◯\";\n      } else {\n        return \"\";\n      }\n    };\n\n    const judgeDelete = (flag: boolean) => {\n      if (flag) {\n        return \"削除\";\n      } else {\n        return \"\";\n      }\n    };\n\n    return {\n      refState,\n      fetchUsers,\n      judgeFlag,\n      judgeDelete,\n      searchUsers,\n    };\n  },\n});\n</script>\n\n<style scoped>\ntr,th,td {\n  border-collapse: collapse;\n  border:1px solid;\n  border-color: #d3d3d3;\n  padding: 10px;\n}\n\n.user-list {\n  margin-left: 60px;\n  margin-right: 60px;\n}\n\n.user-list-wrapper {\n  width: 2500px;\n}\n\n.table-header {\n  background-color: #666666;\n  color: #DCDCDC;\n}\n\n.id {\n  width: 80px;\n}\n\n.user-name, .display-user-id, .phone-number, .self-introduction, .email, .image{\n  width: 200px;\n}\n\n.birthday {\n  width: 110px;\n}\n\n.search-parameter-label {\n  display: block;\n}\n\n</style>\n"]}]}