Q,CAAC,YAAT,GAAwB,KAAK,CAAC,IAAN,CAAW,MAAX,CACrB,QAAD,IAAc,QAAQ,CAAC,OAAT,CAAiB,UAAjB,MAAiC,CADzB,CAAxB;AAGD,OAJD,MAIO;AACL,QAAA,QAAQ,CAAC,YAAT,GAAwB,EAAxB;AACD;AACF,KARD;;AAUA,UAAM,SAAS,GAAI,KAAD,IAAkB;AAClC,MAAA,IAAI,CAAC,cAAD,EAAiB,KAAjB,CAAJ;AACA,MAAA,IAAI,CAAC,gBAAD,EAAmB,KAAnB,CAAJ;AACD,KAHD;;AAKA,UAAM,oBAAoB,GAAI,IAAD,IAAmB;AAC9C,UAAI,IAAI,CAAC,MAAL,KAAgB,CAApB,EAAuB;AACrB,QAAA,IAAI,CAAC,oBAAD,EAAuB,KAAvB,CAAJ;AACD,OAFD,MAEO;AACL,QAAA,IAAI,CAAC,oBAAD,EAAuB,IAAvB,CAAJ;AACD;AACF,KAND;;AAQA,UAAM,SAAS,GAAG,MAAK;AACrB,MAAA,QAAQ,CAAC,OAAT,GAAmB,IAAnB;AACD,KAFD;;AAIA,UAAM,UAAU,GAAG,MAAK;AACtB,MAAA,QAAQ,CAAC,OAAT,GAAmB,KAAnB;AACD,KAFD;;AAIA,IAAA,WAAW,CAAC,MAAK;AACf,MAAA,UAAU,CAAC,KAAK,CAAC,KAAP,CAAV;AACD,KAFU,CAAX;AAIA,IAAA,WAAW,CAAC,MAAK;AACf,MAAA,oBAAoB,CAAC,QAAQ,CAAC,YAAV,CAApB;AACD,KAFU,CAAX;AAIA,IAAA,KAAK,CACH,MAAM,QAAQ,CAAC,OADZ,EAEH,MAAK;AACH,MAAA,IAAI,CAAC,gBAAD,EAAmB,QAAQ,CAAC,OAA5B,CAAJ;AACD,KAJE,CAAL;AAOA;;;;;AAKE;;AACF,UAAM,QAAQ,GAAG,QAAQ,CAAC,MAAK;AAC7B,UAAI,IAAI,GAAG,CAAX;AACA,YAAM,UAAU,GAAG,QAAQ,CAAC,YAAT,CAAsB,MAAzC;AAEA,MAAA,IAAI,GAAG,UAAU,KAAK,CAAf,GAAmB,CAAnB,GAAuB,UAA9B;AACA,MAAA,IAAI,GAAG,UAAU,GAAG,EAAb,GAAkB,EAAlB,GAAuB,UAA9B;AAEA,aAAO,IAAP;AACD,KARwB,CAAzB;AAUA,WAAO;AACL,MAAA,QADK;AAEL,MAAA,SAFK;AAGL,MAAA,UAHK;AAIL,MAAA,SAJK;AAKL,MAAA,UALK;AAML,MAAA;AANK,KAAP;AAQD;;AAtG4B,CAAD,CAA9B","sourcesContent":["\nimport {\n  defineComponent,\n  PropType,\n  reactive,\n  onMounted,\n  computed,\n  watchEffect,\n  watch,\n} from \"vue\";\n\ninterface State {\n  value: string;\n  filteredList: string[];\n  isHover: boolean;\n}\n\nexport default defineComponent({\n  props: {\n    value: {\n      type: String as PropType<string>,\n      required: true,\n    },\n    list: {\n      type: Array as PropType<string[]>,\n      required: true,\n    },\n    isHover: {\n      type: Boolean as PropType<boolean>,\n      required: true,\n    },\n    isExistList: {\n      type: Boolean as PropType<boolean>,\n      required: true,\n    },\n  },\n\n  setup(props, { emit }) {\n    const refState = reactive<State>({\n      value: \"\",\n      filteredList: [],\n      isHover: false,\n    });\n\n    onMounted(() => {\n      refState.value = props.value;\n      listFilter(refState.value);\n    });\n\n    const listFilter = (filterText: string) => {\n      if (filterText !== \"\") {\n        refState.filteredList = props.list.filter(\n          (listItem) => listItem.indexOf(filterText) === 0\n        );\n      } else {\n        refState.filteredList = [];\n      }\n    };\n\n    const emitClick = (value: string) => {\n      emit(\"update:value\", value);\n      emit(\"update:isHover\", false);\n    };\n\n    const filteredListNotEmpty = (list: string[]) => {\n      if (list.length === 0) {\n        emit(\"update:isExistList\", false);\n      } else {\n        emit(\"update:isExistList\", true);\n      }\n    };\n\n    const mouseover = () => {\n      refState.isHover = true;\n    };\n\n    const mouseleave = () => {\n      refState.isHover = false;\n    };\n\n    watchEffect(() => {\n      listFilter(props.value);\n    });\n\n    watchEffect(() => {\n      filteredListNotEmpty(refState.filteredList);\n    });\n\n    watch(\n      () => refState.isHover,\n      () => {\n        emit(\"update:isHover\", refState.isHover);\n      }\n    );\n\n    /*\n       pulldownで要素を表示する個数を算出\n       サジェストのリストの要素が1つであれば、2を返却（2にしないとプルダウンの選択肢として表示されない)\n       2から10であれば、要素数そのままの個数を返却\n       10以上であれば、10を返却\n    */\n    const listSize = computed(() => {\n      let size = 0;\n      const filterSize = refState.filteredList.length;\n\n      size = filterSize === 1 ? 2 : filterSize;\n      size = filterSize > 10 ? 10 : filterSize;\n\n      return size;\n    });\n\n    return {\n      refState,\n      mouseover,\n      mouseleave,\n      emitClick,\n      listFilter,\n      listSize,\n    };\n  },\n});\n"],"sourceRoot":""}]}